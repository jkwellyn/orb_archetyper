source 'http://gemserver.va.opower.it'

<% if project_type == :test -%>
<% File.open(File.join(dir, "templates/gems.txt"), "r") do |infile| -%>
<% infile.each_line do |line| -%>
gem <%= line -%>
<% end -%>
<% end -%>
<% else -%>
# Specify your gem's dependencies in {project_name}.gemspec
gemspec
<% end -%>

